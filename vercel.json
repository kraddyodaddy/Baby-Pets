{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/((?!sitemap\\.xml|robots\\.txt|ads\\.txt).*)",
      "has": [
        {
          "type": "host",
          "value": "babypets.ai"
        }
      ],
      "destination": "https://www.babypets.ai/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/((?!ads\\.txt|sitemap\\.xml|robots\\.txt|.*\\..+).*)",
      "destination": "/index.html"
    }
  ]
}