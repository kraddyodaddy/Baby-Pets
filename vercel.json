{
  "cleanUrls": false,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "babypets.ai"
        }
      ],
      "destination": "https://www.babypets.ai/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/ads.txt",
      "destination": "/ads.txt"
    },
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/((?!ads\\.txt|sitemap\\.xml|robots\\.txt|.*\\..+).*)",
      "destination": "/index.html"
    }
  ]
}